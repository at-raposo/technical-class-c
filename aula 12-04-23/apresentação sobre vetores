#include <stdio.h>
#include <stdlib.h>
#include <math.h>
#include<locale.h>

int main(){
	setlocale(LC_ALL, "PORTUGUESE");
	int var[5];
	
	var[0] = 5+8;
	var[1] = 8/2;
	var[2] = 3*7;
	var[3] = 8-2;
	var[4] = sqrt(16);
	
	printf("\n ##### APRESENTAÇÃO SOBRE VETORES #####\n ");
	printf("\n%d\n",var[0]);
	printf("\n%d\n",var[1]);
	printf("\n%d\n",var[2]);
	printf("\n%d\n",var[3]);
	printf("\n%d\n",var[4]);
		
	return 0;	
	
}
